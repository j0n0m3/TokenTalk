# Ignore node_modules directories in both client and server
node_modules/
client/node_modules/
server/node_modules/

# Ignore build directories for the client-side React app
client/build/
client/dist/

# Ignore environment files
client/.env
server/.env

# Ignore local configuration files
*.local
*.env.*

# Ignore log files
npm-debug.log*
yarn-debug.log*
yarn-error.log*
*.log

# Ignore MacOS system files
.DS_Store

